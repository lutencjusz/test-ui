{
  "cSpell.language": "en, pl",
  "editor.showUnused": false,
  "eslint.format.enable": true,
  "rules": {
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ]
},
"cSpell.words": [
  "Datagrid",
  "antd",
  "atlaskit",
  "backlinks",
  "baseui",
  "browserslist",
  "constaints",
  "dicussed",
  "draggable",
  "fluentui",
  "fontawesome",
  "fortawesome",
  "gojs",
  "headlessui",
  "jsonexport",
  "passmeter",
  "primeflex",
  "primeicons",
  "primereact",
  "rsuite",
  "semibold",
  "strenght",
  "styletron",
  "tailwindcss",
  "tinysoap",
  "todos",
  "umijs",
  "webfonts",
  "wyslij",
  "xmlhttp",
  "zxcvbn"
],
"editor.fontFamily": "JetBrains Mono, 'Courier New', monospace",
"editor.fontSize": 13,
"editor.lineHeight": 19
}
